  "license": "MIT",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^5.1.0",
    "protractor": "^5.1.2",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "protractor-cucumber-framework": "^6.1.1",
    "ts-node": "^3.0.2",
    "@types/node": "~6.0.60"
  },
  "bugs": {
    "url": "https://github.com/kubbafox/protractor-cucumber-typescript-example/issues"
  },
  "homepage": "https://github.com/kubbafox/protractor-cucumber-typescript-example#readme"
