{
  "name": "protractor-cucumber-typescript-example",
  "version": "1.0.0",
  "description": "A simple project to setup E2E Test using protractor, cucumber.js and typescript",
  "main": "index.js",
  "scripts": {
    "debug": "node --inspect --debug-brk ./node_modules/.bin/protractor tmp/conf.js",
    "build": "tsc",
    "pretest": "webdriver-manager update && webdriver-manager start",
    "startTest": "protractor tmp/conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubbafox/protractor-cucumber-typescript-example.git"
  },
